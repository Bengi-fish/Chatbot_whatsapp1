{
  "cleanUrls": true,
  "trailingSlash": false,
  "rewrites": [
    { "source": "/", "destination": "/public/pages/index.html" },
    { "source": "/login.html", "destination": "/public/pages/login.html" },
    { "source": "/reset-password.html", "destination": "/public/pages/reset-password.html" },
    { "source": "/css/:path*", "destination": "/public/css/:path*" },
    { "source": "/js/:path*", "destination": "/public/js/:path*" },
    { "source": "/assets/:path*", "destination": "/public/assets/:path*" }
  ]
}
